{
  "name": "dispatch",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -n NEXT,MCP -c cyan,magenta \"npm run next:dev\" \"npm run mcp:dev\"",
    "next:dev": "next dev",
    "mcp:dev": "tsx watch src/mcp-server/index.ts",
    "mcp:start": "tsx src/mcp-server/index.ts",
    "lint": "eslint .",
    "build": "next build",
    "start": "next start",
    "start:all": "concurrently -k -n NEXT,MCP -c cyan,magenta \"npm run start\" \"npm run mcp:start\"",
    "check": "npm run lint && npm run build && npm test && npm run security:audit",
    "security:audit": "npm audit --audit-level=high",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "setup": "tsx scripts/setup.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.44",
    "@ai-sdk/google": "^3.0.29",
    "@ai-sdk/mcp": "^1.0.21",
    "@ai-sdk/openai": "^3.0.29",
    "@ai-sdk/openai-compatible": "^2.0.30",
    "@ai-sdk/react": "^3.0.88",
    "@auth/drizzle-adapter": "^1.11.1",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "ai": "^6.0.86",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "npm:better-sqlite3-multiple-ciphers@^12.6.2",
    "drizzle-orm": "^0.45.1",
    "next": "^16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@clack/prompts": "^1.0.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
